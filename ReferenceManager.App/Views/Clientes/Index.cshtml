@model IEnumerable<ReferenceManager.App.Models.Cliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Activio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrimerNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SegundoNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrimerApellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SegundoApellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoIdentificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoIdentificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreNegocio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoNegocio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionNegocio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TiempoOperacionAno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TiempoOperacionMese)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TiempoEstablecimientoAno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TiempoEstablecimientoMes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoLocal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoVivienda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoCivil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClaseCiente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkTipoClienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Activio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimerNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SegundoNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimerApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SegundoApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoIdentificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoIdentificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreNegocio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoNegocio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionNegocio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TiempoOperacionAno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TiempoOperacionMese)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TiempoEstablecimientoAno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TiempoEstablecimientoMes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoLocal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoVivienda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoCivil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClaseCiente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkTipoClienteNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
