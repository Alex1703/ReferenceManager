@model IEnumerable<ReferenceManager.App.Models.PlantillaCampo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrdenCampo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EtiquetaCampo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logituud)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Requerido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkCampoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkPlantillaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrdenCampo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EtiquetaCampo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Logituud)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Requerido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkCampoNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkPlantillaNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
